<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.TextArea?>
<?import javafx.scene.layout.BorderPane?>
<?import javafx.scene.layout.HBox?>


<BorderPane xmlns:fx="http://javafx.com/fxml/1" xmlns="http://javafx.com/javafx/11.0.1" fx:controller="application.ParoleSampleController">
   <center>
      <BorderPane prefHeight="269.0" prefWidth="258.0" BorderPane.alignment="CENTER">
         <top>
            <HBox prefHeight="63.0" prefWidth="200.0" BorderPane.alignment="CENTER">
               <children>
                  <TextArea onInputMethodTextChanged="#txtParola" prefHeight="56.0" prefWidth="140.0">
                     <opaqueInsets>
                        <Insets />
                     </opaqueInsets>
                     <HBox.margin>
                        <Insets bottom="20.0" left="5.0" right="5.0" top="10.0" />
                     </HBox.margin>
                  </TextArea>
                  <Button mnemonicParsing="false" onInputMethodTextChanged="#butInserisci" text="inserisci">
                     <HBox.margin>
                        <Insets bottom="5.0" left="5.0" right="5.0" top="20.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </top>
         <left>
            <HBox prefHeight="184.0" prefWidth="251.0" BorderPane.alignment="CENTER">
               <children>
                  <TextArea onInputMethodTextChanged="#txtLista" prefHeight="186.0" prefWidth="174.0">
                     <HBox.margin>
                        <Insets bottom="15.0" left="5.0" right="5.0" top="5.0" />
                     </HBox.margin>
                  </TextArea>
                  <Button mnemonicParsing="false" onInputMethodTextChanged="#butResetta" prefHeight="27.0" prefWidth="77.0" text="resetta">
                     <HBox.margin>
                        <Insets bottom="10.0" left="5.0" right="5.0" top="80.0" />
                     </HBox.margin>
                  </Button>
               </children>
            </HBox>
         </left>
      </BorderPane>
   </center>
</BorderPane>
